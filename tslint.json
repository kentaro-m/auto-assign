{
  "extends": [
      "tslint:latest",
      "tslint-config-prettier",
      "tslint-config-standard"
  ],
  "rules": {
    "interface-name": [true, "never-prefix"],
    "prefer-object-spread": false,
    "semi": "error",
    "no-empty": false,
    "object-literal-sort-keys": false,
    "forin": false,
    "prefer-conditional-expression": false
  }
}