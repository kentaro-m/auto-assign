{
  "name": "auto-assign",
  "version": "1.0.0",
  "description": "A Probot app that adds reviewers to pull requests when pull requests are opened.",
  "author": "Kentaro Matsushita <kentaro.m811@gmail.com>",
  "license": "ISC",
  "repository": "https://github.com/kentaro-m/auto-assign.git",
  "homepage": "https://github.com/kentaro-m/auto-assign",
  "bugs": "https://github.com/kentaro-m/auto-assign/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "nodemon",
    "start": "probot run ./lib/index.js",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "coverage": "jest --coverage",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage",
    "prepare": "husky install"
  },
  "dependencies": {
    "@octokit/webhooks": "7.24.3",
    "@types/lodash": "4.17.20",
    "@types/node": "14.18.63",
    "lodash": "4.17.21",
    "probot": "13.4.7",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@types/jest": "27.5.2",
    "@typescript-eslint/eslint-plugin": "8.46.0",
    "@typescript-eslint/parser": "8.46.0",
    "eslint": "9.37.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "lint-staged": "13.3.0",
    "nodemon": "2.0.22",
    "prettier": "3.6.2",
    "smee-client": "1.2.5",
    "ts-jest": "29.4.4"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  }
}
